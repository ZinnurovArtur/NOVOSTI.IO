<resources>
    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="textColor">#000000</item>
        <item name="metaTextColor">#606060</item>

        <item name="cardview">#FFFFFF</item>

        <item name="bgColor">#ffffff</item>
        <item name="fRameColor">@color/sweetPink</item>
        <item name="icons">@color/RoyalPurple</item>
    </style>



    <style name="DarkTheme" parent="AppTheme">
        <item name="textColor">#ffffff</item>
        <item name="metaTextColor">#aeaeae</item>
        <item name="cardview">#989898</item>
        <item name="bgColor">#000000</item>
        <item name="fRameColor">@color/RoyalPurple</item>

        <item name="icons">@color/sweetPink</item>
    </style>

    <style name= "AppTheme2" parent="Theme.AppCompat.Light.NoActionBar">

        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
    </style>

</resources>